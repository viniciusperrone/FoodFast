{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "assets/*": ["src/assets/*"],
      "global/*": ["src/global/*"],
      "@utils/*": ["src/utils/*"],
      "@components/*": ["src/components/*"],
      "@hooks/*": ["src/hooks/*"],
      "@pages/*": ["src/pages/*"],
      "@server/*": ["src/server/*"],
      "@services/*": ["src/services/*"],
      "@shared/*": ["src/shared/*"],
      "@style/*": ["src/style/*"]
    },
  }
}
